extends layout

block content
	h1= title
	p Welcome to #{title}
	.container
		.span10
			h1 Subir imagenes en nodejs
			form( action="/upload", method="post", id="upload-form", enctype="multipart/form-data" )
				fieldset
					label Selecciona una imagen
					input( type="file", id="image", name="image" )
					br
					button( type="submit", class="btn btn-danger" ) Subir